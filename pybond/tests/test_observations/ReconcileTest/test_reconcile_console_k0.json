    "what": [
        "--- reference", 
        "+++ current", 
        "@@ -2,6 +2,6 @@", 
        " [", 
        " {", 
        "    \"__spy_point__\" : \"point 1\",", 
        "-   val\" : 12345", 
        "+   val\" : abcde", 
        " }", 
        " ]", 
        ""
    ]
},
{
    "__spy_point__": "bond_reconcile._print", 
    "what": "There were differences in observations for test 1: "
},

    "cmd": "kdiff3 -m \"/tmp/bond_tmp_random.ref\" --L1 \"test 1_REFERENCE\" \"/tmp/bond_tmp_random.curr\" --L2 \"test 1_CURRENT\"  -o \"/tmp/bond_tmp_random.merged\""
},
